         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtNumber1" layoutX="42.0" layoutY="106.0" promptText="First number" />
      <TextField fx:id="txtNumber2" layoutX="42.0" layoutY="167.0" promptText="Second number" />
